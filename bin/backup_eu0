#!/usr/bin/env python
import os.path
import shutil
import backupcmds

def okfn():
    base = '/home/okfn/backup'
    backupcmds.clean_directory(base)
    backupcmds.okfn_wikis(base)
    backupcmds.okfn_www(base)
    backupcmds.backup_etc_repo(base)
    backupcmds.backup_ckan(base)

if __name__ == '__main__':
    okfn()
