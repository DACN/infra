<VirtualHost *:80>
    ServerName it.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/it.ckan.net/pyenv/bin/it.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess it.ckan.net display-name=it.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/it.ckan.net.error.log
    CustomLog /var/log/apache2/it.ckan.net.custom.log combined
</VirtualHost>
