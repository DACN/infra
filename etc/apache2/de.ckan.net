<VirtualHost *:80>
    ServerName de.ckan.net
    ServerAlias data.oddir.de
    ServerAlias katalog.offenedaten.de
    ServerAlias ckan.de
    ServerAlias www.ckan.de
    ServerAlias offenedaten.de
    ServerAlias www.offenedaten.de

    WSGIScriptAlias / /home/okfn/var/srvc/de.ckan.net/pyenv/bin/de.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On

    ErrorLog /var/log/apache2/de.ckan.net.error.log
    CustomLog /var/log/apache2/de.ckan.net.custom.log combined
</VirtualHost>
