<VirtualHost *:80>
    ServerName fr.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/fr.ckan.net/pyenv/bin/fr.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess fr.ckan.net display-name=fr.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/fr.ckan.net.error.log
    CustomLog /var/log/apache2/fr.ckan.net.custom.log combined
</VirtualHost>
