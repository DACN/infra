<VirtualHost *:80>
    ServerName ww.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/ww.ckan.net/pyenv/bin/ww.ckan.net.py
    WSGIPassAuthorization On
    
    
    <Location /server-status>
      SetHandler server-status

      Order Deny,Allow
      Allow from all
    </Location>

    ErrorLog /var/log/apache2/ww.ckan.net.error.log
    CustomLog /var/log/apache2/ww.ckan.net.custom.log combined
</VirtualHost>


