<VirtualHost *:80>
    ServerName bg.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/bg.ckan.net/pyenv/bin/bg.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess bg.ckan.net display-name=bg.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/bg.ckan.net.error.log
    CustomLog /var/log/apache2/bg.ckan.net.custom.log combined
</VirtualHost>
