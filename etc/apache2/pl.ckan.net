<VirtualHost *:80>
    ServerName pl.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/pl.ckan.net/pyenv/bin/pl.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess pl.ckan.net display-name=pl.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/pl.ckan.net.error.log
    CustomLog /var/log/apache2/pl.ckan.net.custom.log combined
</VirtualHost>
