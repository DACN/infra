<VirtualHost *:80>
  ServerName db.publicdomainworks.net
  <Proxy *>
	Allow from .publicdomainworks.net
  </Proxy>
  ProxyPass / http://localhost:5070/
  ProxyPassReverse / http://localhost:5070/

  ErrorLog /var/log/apache2/db.publicdomainworks.net.error.log
  CustomLog /var/log/apache2/db.publicdomainworks.net.custom.log combined
</VirtualHost>

