<VirtualHost *:80>
  ServerName www.publicdomainworks.net 
  ServerAlias publicdomainworks.net db.publicdomainworks.net

  WSGIScriptAlias / /home/okfn/var/srvc/pdw/pyenv/bin/pylonsapp_modwsgi.py
  WSGIDaemonProcess publicdomainworks.net display-name=PDW processes=2 threads=1 maximum-requests=1000
  WSGIProcessGroup publicdomainworks.net
  # pass authorization info on (needed for e.g. rest apis)
  WSGIPassAuthorization On

  ErrorLog /var/log/apache2/db.publicdomainworks.net.error.log
  CustomLog /var/log/apache2/db.publicdomainworks.net.custom.log combined
</VirtualHost>

<VirtualHost *:80>
  ServerName blog.publicdomainworks.net

  RedirectPermanent / http://blog.okfn.org/taxonomy/okf-projects/public-domain-works/
	
  ErrorLog /var/log/apache2/blog.publicdomainworks.net.error.log
  CustomLog /var/log/apache2/blog.publicdomainworks.net.custom.log combined
</VirtualHost>

<VirtualHost *:80>
  ServerName wiki.publicdomainworks.net
  ServerAlias alpha.publicdomainworks.net

  RedirectPermanent / http://www.publicdomainworks.net/	
</VirtualHost>

