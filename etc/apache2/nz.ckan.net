<VirtualHost *:80>
    ServerName nz.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/nz.ckan.net/pyenv/bin/nz.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess nz.ckan.net display-name=nz.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/nz.ckan.net.error.log
    CustomLog /var/log/apache2/nz.ckan.net.custom.log combined
</VirtualHost>
