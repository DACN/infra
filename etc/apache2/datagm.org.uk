<VirtualHost *:80>
    ServerName datagm.org.uk
    ServerAlias www.datagm.org.uk
    ServerAlias datagm.staging.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/datagm.org.uk/pyenv/bin/datagm.org.uk.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On

    ErrorLog /var/log/apache2/datagm.org.uk.error.log
    CustomLog /var/log/apache2/datagm.org.uk.custom.log combined
</VirtualHost>
