<VirtualHost *:80>
    ServerName cz.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/cz.ckan.net/pyenv/bin/cz.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess cz.ckan.net display-name=cz.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/cz.ckan.net.error.log
    CustomLog /var/log/apache2/cz.ckan.net.custom.log combined
</VirtualHost>
