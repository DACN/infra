<VirtualHost *:80>
    ServerName si.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/si.ckan.net/pyenv/bin/si.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess si.ckan.net display-name=si.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/si.ckan.net.error.log
    CustomLog /var/log/apache2/si.ckan.net.custom.log combined
</VirtualHost>
