<VirtualHost *:80>
    ServerName gr.ckan.net
    ServerAlias el.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/gr.ckan.net/pyenv/bin/gr.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess gr.ckan.net display-name=gr.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/gr.ckan.net.error.log
    CustomLog /var/log/apache2/gr.ckan.net.custom.log combined
</VirtualHost>
