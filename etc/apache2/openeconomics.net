<VirtualHost *:80>
    ServerName openeconomics.net
    ServerAlias openeconomics.net www.openeconomics.net

    SetHandler python-program
    PythonPath "['/home/okfn/var/srvc/openeconomics.net/pyenv/bin'] + sys.path"
    PythonHandler pylonsapp
    PythonOption paste.ini /home/okfn/var/srvc/openeconomics.net/openeconomics.ini

    ErrorLog /var/log/apache2/openeconomics.net.error.log
    CustomLog /var/log/apache2/openeconomics.net.log combined
</VirtualHost>

