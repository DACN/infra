<VirtualHost *:80>
	ServerName openeconomics.net
  	ServerAlias openeconomics.net www.openeconomics.net
	<Proxy *>
		Allow from .openeconomics.net
	</Proxy>
	ProxyPass / http://localhost:8081/
	ProxyPassReverse / http://localhost:8081/
  SetOutputFilter  proxy-html
  ProxyHTMLURLMap http://localhost:8081 http://www.openeconomics.net
	ErrorLog /var/log/apache2/openeconomics.net.error.log
	CustomLog /var/log/apache2/openeconomics.net.log combined
</VirtualHost>

