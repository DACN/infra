<VirtualHost *:80>
    ServerName be.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/be.ckan.net/pyenv/bin/be.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess be.ckan.net display-name=be.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/be.ckan.net.error.log
    CustomLog /var/log/apache2/be.ckan.net.custom.log combined
</VirtualHost>
