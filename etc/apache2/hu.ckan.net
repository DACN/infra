<VirtualHost *:80>
    ServerName hu.ckan.net

    WSGIScriptAlias / /home/okfn/var/srvc/hu.ckan.net/pyenv/bin/hu.ckan.net.py
    # pass authorization info on (needed for rest api)
    WSGIPassAuthorization On
    WSGIDaemonProcess hu.ckan.net display-name=hu.ckan.net processes=5 threads=2 maximum-requests=1000

    ErrorLog /var/log/apache2/hu.ckan.net.error.log
    CustomLog /var/log/apache2/hu.ckan.net.custom.log combined
</VirtualHost>
