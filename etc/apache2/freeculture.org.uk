<VirtualHost *:80>
    ServerName freeculture.org.uk
    ServerAlias www.freeculture.org.uk
    # DocumentRoot /home/fcuk/svnroot/fcuk/trunk/projects/freeculture.org.uk/public

    Alias /moinhtdocs/ "/usr/share/moin/htdocs/"
    # Alias /moinhtdocs/ "/home/fcuk/svnroot/fcuk/trunk/sysadmin/moin_theme/htdocs/"
    Alias /images /home/fcuk/svnroot/fcuk/trunk/projects/freeculture.org.uk/public/images
    Alias /css /home/fcuk/svnroot/fcuk/trunk/projects/freeculture.org.uk/public/css
    ScriptAlias / "/home/fcuk/var/moin/fcuk_public_wiki/moin.cgi/"
    # ScriptAlias / "/home/fcuk/var/moin/fcuk_public_wiki/moin.fcg/"
    
    <DirectoryMatch ^.*/\.svn>
        deny from all
    </DirectoryMatch>
        
    ErrorLog /home/fcuk/var/log/apache2/freeculture.org.uk.error.log
    CustomLog /home/fcuk/var/log/apache2/freeculture.org.uk.log combined
</VirtualHost>
